  },
  "homepage": "https://github.com/ahdinosaur/t-range#readme",
  "devDependencies": {
    "browserify": "^13.0.0",
    "budo": "^8.1.0",
    "disc": "^1.3.2",
    "envify": "^3.4.0",
    "gh-pages": "^0.11.0",
    "npm-run-all": "^1.6.0",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "virtual-dom": "^2.1.1",
    "virtual-raf": "^3.0.0",
    "watchify": "^3.7.0"
