{
  "name": "t-range",
  "version": "0.0.0",
  "description": "tcomb type for number between max and min",
  "main": "index.js",
  "scripts": {
    "test": "tape test.js",
    "start": "budo example:bundle.js -d example --live -- -d",
    "bundle": "browserify example -g envify -g uglifyify",
